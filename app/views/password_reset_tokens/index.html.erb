<p style="color: green"><%= notice %></p>

<h1>Password reset tokens</h1>

<div id="password_reset_tokens">
  <% @password_reset_tokens.each do |password_reset_token| %>
    <%= render password_reset_token %>
    <p>
      <%= link_to "Show this password reset token", password_reset_token %>
    </p>
  <% end %>
</div>

<%= link_to "New password reset token", new_password_reset_token_path %>
